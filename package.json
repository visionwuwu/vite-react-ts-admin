{
  "name": "vite-react-ts-admin",
  "description": "使用vite2、react17+、typescript搭建后台管理系统脚手架",
  "version": "0.0.0",
  "author": {
    "name": "Visionwuwu",
    "email": "2021664244@qq.com"
  },
  "private": "false",
  "homepage": "",
  "license": "Apache-2.0",
  "keywords": [
    "Visionwuwu",
    "vite-react-ts",
    "antd",
    "React",
    "TypeScript",
    "vite2",
    "后台管理系统"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "clean": "rimraf ./build",
    "dev": "vite",
    "build": "tsc && vite build",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "lessc ./src/styles/index.less ./build/index.css",
    "serve": "vite preview",
    "lint:fix": "eslint src/**/*.{ts,tsx,js} --fix --no-error-on-unmatched-pattern --max-warnings 10",
    "lint:prettier": "prettier src/**/*.{css,less,scss,md} --write --ignore-unknown",
    "lint": "npm run lint:prettier && npm run lint:fix",
    "lint:all": "prettier --write . && eslint --fix .",
    "test:unit": "cross-env CI=true jest",
    "test:nowatch": "cross-env CI=true jest",
    "test:ci": "npm run lint && npm run test:nowatch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.0.20",
    "antd": "^4.12.3",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "clipboard": "^2.0.6",
    "countup.js": "^2.0.7",
    "js-cookie": "^2.2.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "react": "^17.0.0",
    "react-countup": "^4.3.3",
    "react-custom-scrollbars": "^4.2.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^17.0.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "screenfull": "^5.1.0",
    "vite-plugin-importer": "^0.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "@types/classnames": "^2.2.11",
    "@types/clipboard": "^2.0.1",
    "@types/jest": "^26.0.20",
    "@types/js-cookie": "^2.2.6",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.0",
    "@types/react-countup": "^4.3.0",
    "@types/react-custom-scrollbars": "^4.0.7",
    "@types/react-document-title": "^2.0.4",
    "@types/react-dom": "^17.0.0",
    "@types/react-loadable": "^5.5.4",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@vitejs/plugin-react-refresh": "^1.1.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "less": "^4.1.1",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "vite": "^2.0.1",
    "vite-plugin-mock": "^2.2.0",
    "vite-plugin-style-import": "^0.8.1"
  }
}
